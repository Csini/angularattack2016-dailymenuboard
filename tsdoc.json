		"parser"   : "gfm",
		"hardwrap" : true
	},
	"tsdoc":{
		"source"		:"/ent/workspacesM/dmb/angularattack2016-dailymenuboard/src/",
		"destination"	:"/ent/workspacesM/dmb/angularattack2016-dailymenuboard/docs",
		"tutorials"		:"",
		"systemName"	: "angularattack2016-dailymenuboard",
		"footer"		: "",
		"copyright"		: "angularattack2016-dailymenuboard Copyright Â© 2016 Data.",
		"outputSourceFiles" : true,
		"commentsOnly": true
	}
}
